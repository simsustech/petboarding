{
  "name": "@petboarding/docs",
  "productName": "Petboarding",
  "version": "0.1.0",
  "description": "Documentation for Petboarding",
  "license": "UNLICENSED",
  "author": "simsustech",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js"
    },
    "./fastify-ssr-plugin": {
      "import": "./dist/ssr/server/fastify-ssr-plugin.mjs"
    },
    "./hooks": {
      "import": "./dist/ssr/server/virtual_vitrify-hooks.mjs"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "dev": "vitrify dev",
    "dev:ssr": "vitrify dev -m ssr",
    "build": "run-s build:ssg gzip",
    "build:csr": "vitrify build",
    "build:ssr": "vitrify build -m ssr",
    "build:ssg": "vitrify build -m ssg",
    "gzip": "gzip -k -r ./dist/static/*.mjs",
    "test": "vitrify test",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format:check": "prettier --check .",
    "format:write": "prettier --write ."
  },
  "dependencies": {
    "@fastify/middie": "9.0.2",
    "@fastify/static": "8.0.1"
  },
  "devDependencies": {
    "@quasar/extras": "1.16.12",
    "@simsustech/quasar-components": "^0.10.4",
    "@typescript-eslint/eslint-plugin": "8.7.0",
    "@typescript-eslint/parser": "8.7.0",
    "@vitejs/plugin-vue": "5.1.4",
    "@vitrify/tools": "^0.2.0",
    "@vue/reactivity": "^3.5.8",
    "@vue/runtime-core": "^3.5.8",
    "@vue/runtime-dom": "^3.5.8",
    "@vue/server-renderer": "^3.5.8",
    "@vue/shared": "^3.5.8",
    "critters": "0.0.24",
    "devcert": "1.2.2",
    "eslint": "9.11.1",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-prettier-vue": "5.0.0",
    "eslint-plugin-vue": "9.28.0",
    "fastify": "5.0.0",
    "npm-run-all": "4.1.5",
    "quasar": "2.17.0",
    "typescript": "5.6.2",
    "typescript-eslint": "^8.7.0",
    "vite": "^5.4.7",
    "vite-plugin-pwa": "^0.20.5",
    "vitrify": "^0.16.0",
    "vue": "3.5.8",
    "vue-eslint-parser": "^9.4.3",
    "vue-router": "4.4.5"
  }
}
