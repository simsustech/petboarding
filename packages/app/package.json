{
  "private": true,
  "name": "@petboarding/app",
  "version": "0.1.0",
  "description": "Petboarding App",
  "license": "ELv2",
  "author": "simsustech",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js"
    },
    "./fastify-ssr-plugin": {
      "import": "./dist/ssr/server/fastify-ssr-plugin.mjs"
    },
    "./hooks": {
      "import": "./dist/ssr/server/virtual_vitrify-hooks.mjs"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "dev": "vitrify dev",
    "dev:ssr": "vitrify dev -m ssr",
    "build": "run-s build:csr build:ssr",
    "build:csr": "vitrify build",
    "build:ssr": "vitrify build -m ssr",
    "build:ssg": "vitrify build -m ssg",
    "generate:types": "vue-tsc -p tsconfig.types.json",
    "test": "vitrify test",
    "lint": "eslint --ext .vue --ext .ts src",
    "lint:fix": "eslint --ext .vue --ext .ts src --fix",
    "format:check": "prettier --check .",
    "format:write": "prettier --write ."
  },
  "dependencies": {
    "@fastify/middie": "8.3.0",
    "@fastify/static": "6.10.2",
    "@quasar/extras": "1.16.5",
    "@quasar/quasar-ui-qcalendar": "4.0.0-beta.16",
    "@simsustech/quasar-components": "^0.4.4",
    "@trpc/client": "^10.37.1",
    "@trpc/server": "^10.37.1",
    "@vueuse/core": "^10.3.0",
    "abort-controller": "^3.0.0",
    "browser-image-resizer": "^2.4.1",
    "date-holidays": "^3.21.3",
    "html2pdf.js": "0.10.1",
    "lionel-oauth-client": "^0.5.0",
    "node-fetch": "^3.3.2",
    "quasar": "2.12.5",
    "use-trpc": "2.0.0-beta.1",
    "vite-plugin-pwa": "^0.16.5",
    "vue": "3.3.4",
    "vue-router": "4.2.4",
    "ws": "^8.13.0"
  },
  "devDependencies": {
    "@petboarding/api": "^0.1.0",
    "@typescript-eslint/eslint-plugin": "6.4.0",
    "@typescript-eslint/parser": "6.4.0",
    "@vitejs/plugin-vue": "4.3.2",
    "critters": "0.0.20",
    "devcert": "1.2.2",
    "eslint": "8.47.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier-vue": "4.2.0",
    "eslint-plugin-vue": "9.17.0",
    "fastify": "4.21.0",
    "icon-gen": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "5.1.6",
    "vite": "4.4.9",
    "vitrify": "^0.13.2",
    "vue-tsc": "^1.8.8",
    "workbox-window": "^7.0.0"
  }
}
