{
  "private": true,
  "name": "@petboarding/app",
  "version": "0.1.0",
  "description": "Petboarding App",
  "license": "ELv2",
  "author": "simsustech",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js"
    },
    "./fastify-ssr-plugin": {
      "import": "./dist/ssr/server/fastify-ssr-plugin.mjs"
    },
    "./hooks": {
      "import": "./dist/ssr/server/virtual_vitrify-hooks.mjs"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "dev": "vitrify dev",
    "dev:ssr": "vitrify dev -m ssr",
    "build": "run-s build:ssr minify:ssr:server minify:ssr:client",
    "build:csr": "vitrify build",
    "build:ssr": "vitrify build -m ssr",
    "build:ssg": "vitrify build -m ssg",
    "minify:csr": "vitrify minify ./dist/csr",
    "minify:ssr:server": "vitrify minify ./dist/ssr/server",
    "minify:ssr:client": "vitrify minify ./dist/ssr/client",
    "minify:ssg": "vitrify minify ./dist/static",
    "minify:fastify": "vitrify minify ./dist/server",
    "generate:types": "vue-tsc -p tsconfig.types.json",
    "test": "vitrify test",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format:check": "prettier --check .",
    "format:write": "prettier --write ."
  },
  "dependencies": {
    "@fastify/middie": "9.0.3",
    "@fastify/static": "8.1.1",
    "uqr": "^0.1.2"
  },
  "devDependencies": {
    "@iconify-json/flagpack": "^1.2.2",
    "@iconify-json/mdi": "^1.2.3",
    "@modular-api/fastify-checkout": "^0.4.20",
    "@modular-api/quasar-components": "^0.3.17",
    "@petboarding/api": "link:../api",
    "@quasar/extras": "1.16.17",
    "@quasar/quasar-ui-qcalendar": "4.1.2",
    "@simsustech/quasar-components": "^0.11.13",
    "@trpc/client": "^11.1.2",
    "@trpc/server": "^11.1.2",
    "@typescript-eslint/eslint-plugin": "8.32.1",
    "@typescript-eslint/parser": "8.32.1",
    "@vitejs/plugin-vue": "5.2.4",
    "@vue/reactivity": "^3.5.13",
    "@vue/runtime-core": "^3.5.13",
    "@vue/runtime-dom": "^3.5.13",
    "@vue/server-renderer": "^3.5.13",
    "@vue/shared": "^3.5.13",
    "@vueuse/core": "^13.1.0",
    "beasties": "^0.3.3",
    "browser-image-resizer": "^2.4.1",
    "date-fns": "^4.1.0",
    "date-holidays": "^3.24.3",
    "devcert": "1.2.2",
    "eslint": "9.26.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-prettier-vue": "5.0.0",
    "eslint-plugin-vue": "10.1.0",
    "fastify": "5.3.2",
    "icon-gen": "^5.0.0",
    "lionel-oauth-client": "npm:@stefanvh/lionel-oauth-client@^0.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.5.3",
    "quasar": "2.18.1",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.32.1",
    "unocss-preset-quasar": "^0.2.1",
    "use-trpc": "2.0.0-beta.1",
    "vite": "^6.3.5",
    "vite-plugin-pwa": "^1.0.0",
    "vitrify": "^0.22.0",
    "vue": "3.5.13",
    "vue-eslint-parser": "^10.1.3",
    "vue-router": "4.5.1",
    "vue-tsc": "^2.2.10",
    "workbox-window": "^7.3.0",
    "ws": "^8.18.2"
  }
}
