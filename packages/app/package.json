{
  "private": true,
  "name": "@petboarding/app",
  "version": "0.1.0",
  "description": "Petboarding App",
  "license": "ELv2",
  "author": "simsustech",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "import": "./dist/index.js"
    },
    "./fastify-ssr-plugin": {
      "import": "./dist/ssr/server/fastify-ssr-plugin.mjs"
    },
    "./hooks": {
      "import": "./dist/ssr/server/virtual_vitrify-hooks.mjs"
    }
  },
  "files": [
    "dist/"
  ],
  "scripts": {
    "dev": "vitrify dev",
    "dev:ssr": "vitrify dev -m ssr",
    "build": "run-s build:csr build:ssr",
    "build:csr": "vitrify build",
    "build:ssr": "vitrify build -m ssr",
    "build:ssg": "vitrify build -m ssg",
    "generate:types": "vue-tsc -p tsconfig.types.json",
    "test": "vitrify test",
    "lint": "eslint --ext .vue --ext .ts src",
    "lint:fix": "eslint --ext .vue --ext .ts src --fix",
    "format:check": "prettier --check .",
    "format:write": "prettier --write ."
  },
  "dependencies": {
    "@fastify/middie": "8.3.0",
    "@fastify/static": "6.12.0",
    "@quasar/quasar-ui-qcalendar": "4.0.0-beta.16",
    "@simsustech/quasar-components": "^0.4.4",
    "@trpc/client": "^10.44.1",
    "@trpc/server": "^10.44.1",
    "@vueuse/core": "^10.6.1",
    "abort-controller": "^3.0.0",
    "browser-image-resizer": "^2.4.1",
    "date-holidays": "^3.23.2",
    "html2pdf.js": "0.10.1",
    "lionel-oauth-client": "^0.5.0",
    "node-fetch": "^3.3.2",
    "quasar": "2.14.0",
    "use-trpc": "2.0.0-beta.1",
    "vite-plugin-pwa": "^0.17.2",
    "vue": "3.3.9",
    "vue-router": "4.2.5",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@petboarding/api": "^0.1.0",
    "@quasar/extras": "1.16.8",
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "@typescript-eslint/parser": "6.13.1",
    "@vitejs/plugin-vue": "4.5.0",
    "critters": "0.0.20",
    "devcert": "1.2.2",
    "eslint": "8.54.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier-vue": "5.0.0",
    "eslint-plugin-vue": "9.18.1",
    "fastify": "4.24.3",
    "icon-gen": "^4.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "5.3.2",
    "vite": "^5.0.2",
    "vitrify": "^0.14.0",
    "vue-tsc": "^1.8.24",
    "workbox-window": "^7.0.0"
  }
}
