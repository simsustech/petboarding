{
  "private": true,
  "name": "@petboarding/api",
  "productName": "Petboarding",
  "version": "0.1.0",
  "description": "Petboarding API",
  "license": "ELv2",
  "author": "simsustech",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts"
    },
    "./zod": {
      "import": "./src/zod/index.ts"
    },
    "./trpc": {
      "types": "./src/trpc/index.ts"
    }
  },
  "files": [
    "dist/",
    "vitrify.config.ts"
  ],
  "scripts": {
    "serve": "node ./dist/server/server.mjs",
    "dev": "vitrify dev -m fastify",
    "build:oidc-interactions": "cd node_modules/@modules-api/oidc-interactions",
    "build:migrations": "tsc -p tsconfig.migrations.json",
    "build:api": "vitrify build -m fastify",
    "build": "run-s build:api build:migrations seed:fake:generate",
    "test": "vitrify test",
    "test:e2e": "playwright test",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "migrate:latest": "node ./dist/migrate.js",
    "seed:fake:generate": "node ./dist/seeds/fake/writeData.js",
    "seed:fake": "node ./dist/seeds/fake.js",
    "seed:test": "node ./dist/seeds/test.js",
    "seed:data": "node ./dist/seeds/data.js",
    "start": "run-s migrate:latest serve"
  },
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "@fastify/middie": "9.0.3",
    "@fastify/static": "8.1.1",
    "@modular-api/api": "^0.5.32",
    "@modular-api/fastify-checkout": "^0.4.19",
    "@modular-api/fastify-oidc": "^0.8.0",
    "@modular-api/oidc-interactions": "^0.6.0",
    "@petboarding/app": "workspace:*",
    "@trpc/client": "^11.1.2",
    "@trpc/server": "^11.1.2",
    "@vitrify/tools": "^0.4.0",
    "bcryptjs": "^3.0.2",
    "compress-tag": "^3.0.0",
    "date-fns": "^4.1.0",
    "date-holidays": "^3.24.2",
    "fastify": "^5.3.2",
    "handlebars": "^4.7.8",
    "kysely": "^0.28.2",
    "npm-run-all": "^4.1.5",
    "otplib": "^12.0.1",
    "pg": "^8.15.6",
    "pg-boss": "^10.2.0",
    "sharp": "^0.34.1",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.7.0",
    "@iconify-json/flagpack": "^1.2.2",
    "@iconify-json/mdi": "^1.2.3",
    "@playwright/test": "1.52.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/pg": "^8.11.14",
    "@vitejs/plugin-vue": "5.2.3",
    "beasties": "^0.3.3",
    "devcert": "1.2.2",
    "eslint": "9.25.1",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "eslint-plugin-vue": "10.1.0",
    "kysely-codegen": "^0.18.3",
    "oidc-provider": "^9.1.1",
    "openid-client": "^6.4.2",
    "prettier": "^3.5.3",
    "quasar": "2.18.1",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.31.1",
    "vite": "6.3.4",
    "vitrify": "^0.20.0",
    "vue": "3.5.13",
    "vue-eslint-parser": "^10.1.3",
    "vue-router": "4.5.1"
  },
  "dependenciesMeta": {
    "@modular-api/fastify-oidc": {
      "injected": true
    }
  }
}
