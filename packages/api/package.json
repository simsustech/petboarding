{
  "private": true,
  "name": "@petboarding/api",
  "productName": "Petboarding",
  "version": "0.1.0",
  "description": "Petboarding API",
  "license": "ELv2",
  "author": "simsustech",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts"
    },
    "./zod": {
      "import": "./src/zod/index.ts"
    },
    "./trpc": {
      "types": "./src/trpc/index.ts"
    }
  },
  "files": [
    "dist/",
    "vitrify.config.ts"
  ],
  "scripts": {
    "serve": "node ./dist/server/server.mjs",
    "dev": "run-s docker-compose start:dev-server ",
    "build": "run-s build:api build:migrations seed:fake:generate minify:fastify",
    "build:migrations": "tsc -p tsconfig.migrations.json",
    "build:api": "vitrify build -m fastify",
    "minify:csr": "vitrify minify ./dist/csr",
    "minify:ssr:server": "vitrify minify ./dist/ssr/server",
    "minify:ssr:client": "vitrify minify ./dist/ssr/client",
    "minify:ssg": "vitrify minify ./dist/static",
    "minify:fastify": "vitrify minify ./dist/server",
    "test": "vitrify test",
    "test:e2e": "playwright test",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "migrate:latest": "node ./dist/migrate.js",
    "seed:fake:generate": "node ./dist/seeds/fake/writeData.js",
    "seed:fake": "node ./dist/seeds/fake.js",
    "seed:test": "node ./dist/seeds/test.js",
    "seed:data": "node ./dist/seeds/data.js",
    "start": "run-s migrate:latest serve",
    "start:dev-server": "NODE_TLS_REJECT_UNAUTHORIZED=0 vitrify dev -m fastify --port 3000",
    "docker-compose": "node ./docker-compose.dev.mjs"
  },
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@fastify/middie": "9.0.3",
    "@fastify/one-line-logger": "^2.0.2",
    "@fastify/static": "8.3.0",
    "@modular-api/api": "^0.6.11",
    "@modular-api/fastify-checkout": "^0.5.4",
    "@modular-api/fastify-oidc": "^0.9.13",
    "@petboarding/app": "workspace:*",
    "@pinia/colada": "0.17.9",
    "@trpc/client": "^11.7.2",
    "@trpc/server": "^11.7.2",
    "@vitrify/tools": "^0.4.4",
    "bcryptjs": "^3.0.3",
    "compress-tag": "^3.0.0",
    "date-fns": "^4.1.0",
    "date-holidays": "^3.26.5",
    "fastify": "^5.6.2",
    "handlebars": "^4.7.8",
    "kysely": "^0.28.8",
    "npm-run-all": "^4.1.5",
    "otplib": "^12.0.1",
    "pg": "^8.16.3",
    "pg-boss": "^12.5.1",
    "pinia": "^3.0.4",
    "sharp": "^0.34.5",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "@iconify-json/flagpack": "^1.2.6",
    "@iconify-json/mdi": "^1.2.3",
    "@playwright/test": "1.58.2",
    "@types/bcryptjs": "^3.0.0",
    "@types/oidc-provider": "^9.5.0",
    "@types/pg": "^8.15.6",
    "@unocss/preset-icons": "^66.5.9",
    "@unocss/preset-wind3": "^66.5.9",
    "@vitejs/plugin-vue": "6.0.2",
    "beasties": "^0.3.5",
    "devcert": "1.2.2",
    "docker-compose": "^1.3.0",
    "eslint": "9.39.1",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "10.6.2",
    "kysely-codegen": "^0.19.0",
    "oidc-provider": "^9.5.2",
    "openid-client": "^6.8.1",
    "prettier": "^3.7.2",
    "quasar": "2.18.6",
    "typescript": "5.9.3",
    "typescript-eslint": "^8.48.0",
    "unocss": "^66.5.9",
    "unocss-preset-quasar": "^0.2.12",
    "vite": "7.2.4",
    "vitrify": "^0.25.4",
    "vue": "3.5.25",
    "vue-eslint-parser": "^10.2.0",
    "vue-router": "4.6.3"
  },
  "dependenciesMeta": {
    "@modular-api/fastify-oidc": {
      "injected": true
    }
  }
}
