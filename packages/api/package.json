{
  "private": true,
  "name": "@petboarding/api",
  "productName": "Petboarding",
  "version": "0.1.0",
  "description": "Petboarding API",
  "license": "ELv2",
  "author": "simsustech",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts"
    },
    "./zod": {
      "import": "./src/zod/index.ts"
    },
    "./trpc": {
      "types": "./src/trpc/index.ts"
    }
  },
  "files": [
    "dist/",
    "vitrify.config.ts"
  ],
  "scripts": {
    "serve": "node ./dist/server/server.mjs",
    "dev": "vitrify dev -m fastify",
    "build:migrations": "tsc -p tsconfig.migrations.json",
    "build:api": "vitrify build -m fastify",
    "build": "run-s build:api build:migrations seed:fake:generate",
    "test": "vitrify test",
    "test:e2e": "playwright test",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "migrate:latest": "node ./dist/migrate.js",
    "seed:fake:generate": "node ./dist/seeds/fake/writeData.js",
    "seed:fake": "node ./dist/seeds/fake.js",
    "seed:test": "node ./dist/seeds/test.js",
    "seed:data": "node ./dist/seeds/data.js",
    "start": "run-s migrate:latest serve"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.1",
    "@fastify/middie": "9.0.2",
    "@fastify/static": "8.0.3",
    "@modular-api/api": "^0.5.15",
    "@modular-api/fastify-checkout": "^0.4.10",
    "@modular-api/fastify-oidc": "^0.6.4",
    "@petboarding/app": "^0.1.0",
    "@trpc/client": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "@vitrify/tools": "^0.2.3",
    "bcrypt": "^5.1.1",
    "compress-tag": "^3.0.0",
    "date-fns": "^4.1.0",
    "date-holidays": "^3.23.13",
    "fastify": "^5.1.0",
    "handlebars": "^4.7.8",
    "kysely": "^0.27.4",
    "npm-run-all": "^4.1.5",
    "otplib": "^12.0.1",
    "pg": "^8.13.1",
    "pg-boss": "^10.1.5",
    "sharp": "^0.33.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.2.0",
    "@playwright/test": "1.49.0",
    "@types/bcrypt": "^5.0.2",
    "@types/pg": "^8.11.10",
    "@vitejs/plugin-vue": "5.2.1",
    "critters": "0.0.25",
    "devcert": "1.2.2",
    "eslint": "9.15.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-vue": "9.31.0",
    "kysely-codegen": "^0.17.0",
    "oidc-provider": "^8.6.0",
    "openid-client": "^6.1.4",
    "quasar": "2.17.4",
    "typescript": "5.7.2",
    "typescript-eslint": "^8.16.0",
    "vite": "^6.0.1",
    "vitrify": "^0.16.4",
    "vue": "3.5.13",
    "vue-eslint-parser": "^9.4.3",
    "vue-router": "4.5.0"
  },
  "dependenciesMeta": {
    "@modular-api/fastify-oidc": {
      "injected": true
    }
  }
}
