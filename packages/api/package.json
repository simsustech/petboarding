{
  "private": true,
  "name": "@petboarding/api",
  "productName": "Petboarding",
  "version": "0.1.0",
  "description": "Petboarding API",
  "license": "ELv2",
  "author": "simsustech",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts"
    },
    "./zod": {
      "import": "./src/zod/index.ts"
    },
    "./trpc": {
      "types": "./src/trpc/index.ts"
    }
  },
  "files": [
    "dist/",
    "vitrify.config.ts"
  ],
  "scripts": {
    "serve": "node ./dist/server/server.mjs",
    "dev": "vitrify dev -m fastify",
    "build:migrations": "tsc -p tsconfig.migrations.json",
    "build:api": "vitrify build -m fastify",
    "build": "run-s build:api build:migrations seed:fake:generate",
    "test": "vitrify test",
    "test:e2e": "playwright test",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "migrate:latest": "node ./dist/migrate.js",
    "seed:fake:generate": "node ./dist/seeds/fake/writeData.js",
    "seed:fake": "node ./dist/seeds/fake.js",
    "seed:test": "node ./dist/seeds/test.js",
    "seed:data": "node ./dist/seeds/data.js",
    "start": "run-s migrate:latest serve"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.2",
    "@fastify/middie": "9.0.3",
    "@fastify/static": "8.1.0",
    "@modular-api/api": "^0.5.20",
    "@modular-api/fastify-checkout": "^0.4.13",
    "@modular-api/fastify-oidc": "^0.6.4",
    "@petboarding/app": "workspace:*",
    "@trpc/client": "^10.45.2",
    "@trpc/server": "^10.45.2",
    "@vitrify/tools": "^0.3.2",
    "bcrypt": "^5.1.1",
    "compress-tag": "^3.0.0",
    "date-fns": "^4.1.0",
    "date-holidays": "^3.23.20",
    "fastify": "^5.2.1",
    "handlebars": "^4.7.8",
    "kysely": "^0.27.5",
    "npm-run-all": "^4.1.5",
    "otplib": "^12.0.1",
    "pg": "^8.13.1",
    "pg-boss": "^10.1.6",
    "sharp": "^0.33.5",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.4.0",
    "@playwright/test": "1.50.1",
    "@types/bcrypt": "^5.0.2",
    "@types/pg": "^8.11.11",
    "@vitejs/plugin-vue": "5.2.1",
    "critters": "0.0.25",
    "devcert": "1.2.2",
    "eslint": "9.19.0",
    "eslint-config-prettier": "10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "eslint-plugin-vue": "9.32.0",
    "kysely-codegen": "^0.17.0",
    "oidc-provider": "^8.6.1",
    "openid-client": "^6.1.7",
    "quasar": "2.17.7",
    "typescript": "5.7.3",
    "typescript-eslint": "^8.23.0",
    "vite": "6.0.11",
    "vitrify": "^0.17.16",
    "vue": "3.5.13",
    "vue-eslint-parser": "^9.4.3",
    "vue-router": "4.5.0"
  },
  "dependenciesMeta": {
    "@modular-api/fastify-oidc": {
      "injected": true
    }
  }
}
