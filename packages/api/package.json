{
  "private": true,
  "name": "@petboarding/api",
  "productName": "Petboarding",
  "version": "0.1.0",
  "description": "Petboarding API",
  "license": "ELv2",
  "author": "simsustech",
  "type": "module",
  "exports": {
    ".": {
      "types": "./src/index.ts"
    },
    "./zod": {
      "import": "./src/zod/index.ts"
    },
    "./trpc": {
      "types": "./src/trpc/index.ts"
    }
  },
  "files": [
    "dist/",
    "vitrify.config.ts"
  ],
  "scripts": {
    "serve": "node ./dist/server/server.mjs",
    "dev": "vitrify dev -m fastify",
    "build": "run-s build:api build:migrations seed:fake:generate minify:fastify",
    "build:migrations": "tsc -p tsconfig.migrations.json",
    "build:api": "vitrify build -m fastify",
    "minify:csr": "vitrify minify ./dist/csr",
    "minify:ssr:server": "vitrify minify ./dist/ssr/server",
    "minify:ssr:client": "vitrify minify ./dist/ssr/client",
    "minify:ssg": "vitrify minify ./dist/static",
    "minify:fastify": "vitrify minify ./dist/server",
    "test": "vitrify test",
    "test:e2e": "playwright test",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "migrate:latest": "node ./dist/migrate.js",
    "seed:fake:generate": "node ./dist/seeds/fake/writeData.js",
    "seed:fake": "node ./dist/seeds/fake.js",
    "seed:test": "node ./dist/seeds/test.js",
    "seed:data": "node ./dist/seeds/data.js",
    "start": "run-s migrate:latest serve"
  },
  "dependencies": {
    "@fastify/cors": "^11.0.1",
    "@fastify/middie": "9.0.3",
    "@fastify/one-line-logger": "^2.0.2",
    "@fastify/static": "8.2.0",
    "@modular-api/api": "^0.6.1",
    "@modular-api/fastify-checkout": "^0.5.1",
    "@modular-api/fastify-oidc": "^0.9.4",
    "@petboarding/app": "workspace:*",
    "@trpc/client": "^11.1.3",
    "@trpc/server": "^11.1.3",
    "@vitrify/tools": "^0.4.1",
    "bcryptjs": "^3.0.2",
    "compress-tag": "^3.0.0",
    "date-fns": "^4.1.0",
    "date-holidays": "^3.24.3",
    "fastify": "^5.3.3",
    "handlebars": "^4.7.8",
    "kysely": "^0.28.2",
    "npm-run-all": "^4.1.5",
    "otplib": "^12.0.1",
    "pg": "^8.16.0",
    "pg-boss": "^10.2.0",
    "sharp": "^0.34.2",
    "zod": "^3.25.30"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.8.0",
    "@iconify-json/flagpack": "^1.2.2",
    "@iconify-json/mdi": "^1.2.3",
    "@playwright/test": "1.52.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/pg": "^8.15.2",
    "@unocss/preset-icons": "^66.1.2",
    "@unocss/preset-wind3": "^66.1.2",
    "@vitejs/plugin-vue": "5.2.4",
    "beasties": "^0.3.4",
    "devcert": "1.2.2",
    "eslint": "9.27.0",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-vue": "10.1.0",
    "kysely-codegen": "^0.18.5",
    "oidc-provider": "^9.1.1",
    "openid-client": "^6.5.0",
    "prettier": "^3.5.3",
    "quasar": "2.18.1",
    "typescript": "5.8.3",
    "typescript-eslint": "^8.32.1",
    "unocss": "^66.1.2",
    "unocss-preset-quasar": "^0.2.2",
    "vite": "6.3.5",
    "vitrify": "^0.25.0",
    "vue": "3.5.15",
    "vue-eslint-parser": "^10.1.3",
    "vue-router": "4.5.1"
  },
  "dependenciesMeta": {
    "@modular-api/fastify-oidc": {
      "injected": true
    }
  }
}
