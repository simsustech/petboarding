{
  "private": true,
  "name": "@petboarding/api",
  "productName": "Petboarding",
  "version": "0.1.0",
  "description": "Petboarding API",
  "license": "ELv2",
  "author": "simsustech",
  "type": "module",
  "exports": {
    "./zod": {
      "import": "./src/zod/index.ts"
    },
    "./trpc": {
      "types": "./src/trpc/index.ts"
    }
  },
  "typesVersions": {
    "*": {
      "trpc": [
        "./src/trpc/index.ts"
      ],
      "zod": [
        "./src/zod/index.ts"
      ]
    }
  },
  "files": [
    "dist/",
    "vitrify.config.ts"
  ],
  "scripts": {
    "serve": "node ./dist/server/server.mjs",
    "dev": "vitrify dev -m fastify",
    "build:migrations": "tsc -p tsconfig.migrations.json",
    "build:api": "vitrify build -m fastify",
    "build": "run-s build:api build:migrations",
    "test": "vitrify test",
    "test:e2e": "playwright test",
    "lint": "eslint --ext .vue --ext .ts src",
    "lint:fix": "eslint --ext .vue --ext .ts src --fix",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "migrate:latest": "node ./dist/migrate.js",
    "seed:fake:generate": "node ./dist/seeds/fake/writeData.js",
    "seed:fake": "node ./dist/seeds/fake.js",
    "seed:test": "node ./dist/seeds/test.js",
    "seed:data": "node ./dist/seeds/data.js",
    "start": "run-s migrate:latest migrate:latest serve"
  },
  "dependencies": {
    "@fastify/cors": "^8.4.1",
    "@fastify/middie": "8.3.0",
    "@fastify/static": "6.12.0",
    "@modular-api/api": "^0.3.3",
    "@modular-api/fastify-oidc": "^0.2.4",
    "@petboarding/app": "^0.1.0",
    "@trpc/server": "^10.44.1",
    "@vitrify/tools": "^0.1.4",
    "bcrypt": "^5.1.1",
    "compress-tag": "^3.0.0",
    "date-fns": "^2.30.0",
    "date-holidays": "^3.23.2",
    "fastify": "^4.24.3",
    "handlebars": "^4.7.8",
    "js-base64": "^3.7.5",
    "kysely": "^0.26.3",
    "npm-run-all": "^4.1.5",
    "oidc-provider": "^8.4.1",
    "otplib": "^12.0.1",
    "pg": "^8.11.3",
    "quasar": "2.14.0",
    "sharp": "^0.32.6",
    "vue": "3.3.9",
    "vue-router": "4.2.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.3.1",
    "@playwright/test": "1.40.1",
    "@quasar/extras": "1.16.8",
    "@types/bcrypt": "^5.0.2",
    "@types/oidc-provider": "^8.4.3",
    "@types/pg": "^8.10.9",
    "@typescript-eslint/eslint-plugin": "6.13.1",
    "@typescript-eslint/parser": "6.13.1",
    "@vitejs/plugin-vue": "4.5.0",
    "critters": "0.0.20",
    "devcert": "1.2.2",
    "eslint": "8.54.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-prettier-vue": "5.0.0",
    "eslint-plugin-vue": "9.18.1",
    "kysely-codegen": "^0.11.0",
    "typescript": "5.3.2",
    "vite": "^5.0.2",
    "vitrify": "^0.14.0"
  },
  "dependenciesMeta": {
    "@modular-api/fastify-oidc": {
      "injected": true
    }
  }
}
